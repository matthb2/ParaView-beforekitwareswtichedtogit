<ServerManagerConfiguration>
  <ProxyGroup name="filters">
   <SourceProxy name="HaloFinder" class="vtkCosmoHaloFinder" label="Cosmology Halo Finder">
     <InputProperty
        name="Input"
        command="SetInputConnection">
          <ProxyGroupDomain name="groups">
            <Group name="sources"/>
            <Group name="filters"/>
          </ProxyGroupDomain>
          <DataTypeDomain name="input_type">
            <DataType value="vtkUnstructuredGrid"/>
          </DataTypeDomain>
     </InputProperty>
     
     <IntVectorProperty 
        name="pmin" 
        command="Setpmin" 
        number_of_elements="1"
        default_values="10" > 
       <Documentation>
         Minimal number of particles needed before a group is called a halo.
       </Documentation>
     </IntVectorProperty>
     <DoubleVectorProperty 
        name="bb" 
        command="Setbb" 
        number_of_elements="1"
        default_values="0.20" > 
       <Documentation>
         Linking length measured in units of interparticle spacing and is dimensionless.
       </Documentation>
     </DoubleVectorProperty>
     <DoubleVectorProperty 
        name="rL" 
        command="SetrL" 
        label="rL"
        number_of_elements="1"
        default_values="90.140845070423" > 
       <Documentation>
         Physical length of the box.
       </Documentation>
     </DoubleVectorProperty>
     <IntVectorProperty 
        name="Periodic" 
        command="SetPeriodic" 
        number_of_elements="1"
        default_values="1" > 
       <BooleanDomain name="bool"/>
       <Documentation>
       </Documentation>
     </IntVectorProperty>
   </SourceProxy>

