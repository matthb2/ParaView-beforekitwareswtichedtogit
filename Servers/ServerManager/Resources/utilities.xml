        <BooleanDomain name="bool"/>
      </IntVectorProperty>
    </SphereWidgetProxy>

    <PickSphereWidgetProxy name="PickSphereWidgetProxy" class="vtkPickSphereWidget">
      <IntVectorProperty
        name="IgnorePlaceWidgetChanges"
        command="SetIgnorePlaceWidgetChanges"
        number_of_elements="1"
        update_self="1"
        default_values="0" >
        <BooleanDomain name="bool"/>
      </IntVectorProperty>
      <DoubleVectorProperty
        name="CenterInfo"
        command="GetCenter"
        number_of_elements="3"
        update_self="1"
        information_only="1"
        default_values="0.0 0.0 0.0" >
        <SimpleDoubleInformationHelper/>
