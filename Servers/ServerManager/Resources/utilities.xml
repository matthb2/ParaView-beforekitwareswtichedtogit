  </ProxyGroup>

  <ProxyGroup name="animation">
    <AnimationCueProxy name="AnimationCue" class="notused">
      <!-- No server side objects created. All properties are updateself -->
      <IntVectorProperty
         name="TimeMode"
         command="SetTimeMode"
         number_of_elements="1" 
         update_self="1"
         default_values="1">
        <EnumerationDomain name="enum">
          <Entry value="0" text="Normalized" />
          <Entry value="1" text="Relative" />
        </EnumerationDomain>
      </IntVectorProperty>
   
      <DoubleVectorProperty
        name="StartTime"
        command="SetStartTime"
        number_of_elements="1"
        update_self="1"
        default_values="0">
      </DoubleVectorProperty>

      <DoubleVectorProperty
        name="EndTime"
        command="SetEndTime"
        number_of_elements="1"
        update_self="1"
        default_values="0">
      </DoubleVectorProperty>
      
      <ProxyProperty name="AnimatedProxy" 
        command="SetAnimatedProxy"
        clean_command="RemoveAnimatedProxy"
        update_self="1">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>
      </ProxyProperty>

      <StringVectorProperty
        name="AnimatedPropertyName" 
        command="SetAnimatedPropertyName"
        number_of_elements="1"
        update_self="1">
      </StringVectorProperty>

      <StringVectorProperty
        name="AnimatedDomainName"
        command="SetAnimatedDomainName"
        number_of_elements="1"
        update_self="1">
      </StringVectorProperty>

      <IntVectorProperty
        name="AnimatedElement"
        command="SetAnimatedElement"
        number_of_elements="1"
        update_self="1"
        default_values="0">
      </IntVectorProperty>

      <ProxyProperty name="Manipulator"
        command="SetManipulator"
        update_self="1">
        <ProxyGroupDomain name="groups">
          <Group name="animation_manipulators" />
        </ProxyGroupDomain>
      </ProxyProperty>
    </AnimationCueProxy>

    <!--Scene -->
    <AnimationSceneProxy name="AnimationScene" class="notused">
      <!-- No server side objects created. All properties are updateself -->
      <ProxyProperty
        name="RenderModule"
        command="SetRenderModuleProxy"
        update_self="1">
        <ProxyGroupDomain name="groups">
          <Group name="rendermodules" />
        </ProxyGroupDomain>
      </ProxyProperty>
      <IntVectorProperty
        name="AnimatedElement"
        command="SetAnimatedElement"
        number_of_elements="1"
        update_self="1"
