        number_of_elements="3"
        update_self="1"
        information_only="1"
        default_values="0.0 0.0 0.0" >
        <SimpleDoubleInformationHelper/>
      </DoubleVectorProperty>
      <DoubleVectorProperty
        name="PlaceWidget"
        command="PlaceWidget"
        number_of_elements="6"
        update_self="1"
        argument_is_array="1"
