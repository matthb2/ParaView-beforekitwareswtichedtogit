        <ExposedProperties>
          <Property name="Color" />
          <Property name="Opacity" />
          <Property name="FontFamily" />
          <Property name="Bold" />
          <Property name="Italic" />
          <Property name="Shadow" />
          <Property name="FontSize" />
        </ExposedProperties>
      </SubProxy>

      <!--title text property -->
