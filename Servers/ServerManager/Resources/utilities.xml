      </IntVectorProperty>
    </Proxy>

    <XYPlotActorProxy name="XYPlotActor" class="vtkXYPlotActor">
      <InputProperty
        name="Input"
        command="SetInput"
        clean_command="RemoveAllInputs">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>
          <DataTypeDomain name="input_type">
            <DataType value="vtkDataSet"/>
          </DataTypeDomain>
          <InputArrayDomain name="input_array" attribute_type="point"
                            number_of_components="1"/>
      </InputProperty>
      <ProxyProperty
        name="Property"
        command="SetProperty">
        <ProxyGroupDomain name="groups">
          <Group name="properties" />
        </ProxyGroupDomain>
      </ProxyProperty>
      <DoubleVectorProperty
        name="Position"
        command="SetPosition"
        number_of_elements="2"
        default_values="0 0"
        update_self="1">
        <DoubleRangeDomain name="range" />
      </DoubleVectorProperty>
      <DoubleVectorProperty
        name="Position2"
        command="SetPosition2"
        number_of_elements="2"
        default_values="0 0"
        update_self="1">
        <DoubleRangeDomain name="range" />
      </DoubleVectorProperty>
      <IntVectorProperty
        name="Visibility"
        command="SetVisibility"
        number_of_elements="1"
        default_values="1">
        <BooleanDomain name="bool" />
      </IntVectorProperty>
      <IntVectorProperty
        name="PlotPoints"
        command="SetPlotPoints"
        number_of_elements="1"
        default_values="0">
        <BooleanDomain name="bool"/>
      </IntVectorProperty>
      <IntVectorProperty 
        name="PlotLines"
        command="SetPlotLines"
        number_of_elements="1"
        default_values="1">
        <BooleanDomain name="bool" />
      </IntVectorProperty>
      <IntVectorProperty
        name="NumberOfXLabels"
        comamnd="SetNumberOfXLabels"
        number_of_elements="1"
        default_values="5">
        <IntRangeDomain name="range" min="0" max="50"/>
      </IntVectorProperty>
      <IntVectorProperty
        name="NumberOfYLabels"
        command="SetNumberOfYLabels"
        number_of_elements="1"
        default_values="5">
        <IntRangeDomain name="range" min="0" max="50" />
      </IntVectorProperty>
      <DoubleVectorProperty
        name="LegendPosition"
        command="SetLegendPosition"
        number_of_elements="2"
        default_values="0.85 0.75">
      </DoubleVectorProperty>
      <DoubleVectorProperty
        name="LegendPosition2"
        command="SetLegendPosition2"
        number_of_elements="2"
        default_values="0.15 0.20">
      </DoubleVectorProperty>
      <IntVectorProperty
        name="LegendVisibility"
        command="SetLegend"
        number_of_elements="1"
        default_values="0">
        <BooleanDomain name="bool"/>
      </IntVectorProperty>
      <StringVectorProperty
        name="XTitle"
        command="SetXTitle"
        number_of_elements="1">
      </StringVectorProperty>
      <StringVectorProperty
        name="ArrayNames"
        command="AddArrayName"
        update_self="1"
        clean_command="RemoveAllArrayNames"
        repeat_command="1"
        number_of_elements_per_command="1">
        <ArrayListDomain name="array_list" attribute_type="Scalars">
          <RequiredProperties>
            <Property name="Input" function="Input" />
          </RequiredProperties>
        </ArrayListDomain>
      </StringVectorProperty>
      <IntVectorProperty
        name="XValues"
        command="SetXValues"
        number_of_elements="1"
        default_values="0">
        <IntRangeDomain name="xvalues" min="0" max="3" />
      </IntVectorProperty>
    </XYPlotActorProxy>

    <ScalarBarActorProxy name="ScalarBarActor" class="vtkScalarBarActor">
