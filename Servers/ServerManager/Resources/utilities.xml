        <ProxyGroupDomain name="groups">
          <Group name="camera"/>
        </ProxyGroupDomain>
      </ProxyProperty>
      <IntVectorProperty
        name="Visibility"
        command="SetVisibility"
        number_of_elements="1"
        default_values="1">
